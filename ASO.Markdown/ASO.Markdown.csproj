<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\primeicons.ttf" />
    <None Remove="MarkdownInfoFiles\home.md" />
    <None Remove="MarkdownInfoFiles\keyboard.png" />
    <None Remove="MarkdownInfoFiles\question.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="MarkdownInfoFiles\home.md">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AvalonEdit" Version="6.1.3.50" />
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.1343.22" />
    <PackageReference Include="Neo.Markdig.Xaml" Version="1.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\primeicons.ttf" />
    <Resource Include="MarkdownInfoFiles\keyboard.png" />
    <Resource Include="MarkdownInfoFiles\question.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Markdig.Wpf" Version="0.5.0.1" />
  </ItemGroup>

</Project>
