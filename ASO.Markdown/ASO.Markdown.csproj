<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\primeicons.ttf" />
    <None Remove="MarkdownInfoFiles\home.md" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="MarkdownInfoFiles\home.md">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AvalonEdit" Version="6.1.3.50" />
    <PackageReference Include="Markdig" Version="0.30.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\primeicons.ttf" />
  </ItemGroup>

</Project>
